    "prepare.ionic.cosmopatient": "npm run clean && npm run clean.ionic.cosmopatient && npm run build.ionic.cosmopatient",
    "prepare.ionic.cosmopatient.ios": "npm run prepare.ionic.cosmopatient && cd apps/ionic-cosmopatient && npm run cap.add.ios",
    "prepare.ionic.cosmopatient.android": "npm run prepare.ionic.cosmopatient && cd apps/ionic-cosmopatient && npm run cap.add.android",
    "open.ionic.cosmopatient.ios": "cd apps/ionic-cosmopatient && npm run cap.ios",
    "open.ionic.cosmopatient.android": "cd apps/ionic-cosmopatient && npm run cap.android",
    "sync.ionic.cosmopatient": "cd apps/ionic-cosmopatient && npm run cap.copy",
    "clean.ionic.cosmopatient": "cd apps/ionic-cosmopatient && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json"
  },
